{
  "name": "tracil-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered clinical data lineage platform",
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=frontend",
    "start": "npm run start --workspace=frontend",
    "lint": "npm run lint --workspace=frontend",
    "test": "npm run test --workspace=frontend",
    "frontend:dev": "npm run dev --workspace=frontend",
    "frontend:build": "npm run build --workspace=frontend",
    "frontend:test": "npm run test --workspace=frontend",
    "backend:dev": "cd backend && echo 'AI Developer: Set up your Python environment and run your FastAPI server'",
    "backend:install": "cd backend && echo 'AI Developer: Install your Python dependencies (pip install -r requirements.txt)'"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
